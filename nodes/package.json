{
  "name": "n8n-nodes-opflow",
  "version": "1.0.0",
  "description": "Custom n8n nodes for OpFlow DevOps automation",
  "keywords": [
    "n8n",
    "devops",
    "automation",
    "jenkins",
    "vault",
    "artifactory",
    "ansible",
    "mcp"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "npx jest --coverage",
    "test:watch": "npx jest --watch",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "nodes": [
      "dist/nodes/Jenkins/Jenkins.node.js",
      "dist/nodes/Vault/Vault.node.js",
      "dist/nodes/Artifactory/Artifactory.node.js",
      "dist/nodes/AnsibleAWX/AnsibleAWX.node.js",
      "dist/nodes/MCP/MCP.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/nock": "^10.0.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nock": "^14.0.10",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0",
    "node-fetch": "^3.0.0"
  }
}
