# Database Configuration
DB_TYPE=postgres
DB_POSTGRESDB_HOST=localhost
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n_user
DB_POSTGRESDB_PASSWORD=n8n_password

# n8n Configuration
N8N_PORT=5678
N8N_PROTOCOL=http
N8N_HOST=localhost
N8N_PATH=/
N8N_EDITOR_BASE_URL=http://localhost:5678

# Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin_password

# Keycloak SSO Configuration
N8N_AUTH_IDENTITY_PROVIDER=keycloak
N8N_AUTH_KEYCLOAK_REALM=n8n
N8N_AUTH_KEYCLOAK_AUTH_SERVER_URL=http://localhost:8080
N8N_AUTH_KEYCLOAK_CLIENT_ID=n8n-client
N8N_AUTH_KEYCLOAK_CLIENT_SECRET=n8n-client-secret

# Security
N8N_ENCRYPTION_KEY=your_32_character_encryption_key_here

# Jenkins Configuration (for Jenkins node)
JENKINS_URL=http://jenkins:8080
JENKINS_USERNAME=jenkins_user
JENKINS_API_TOKEN=jenkins_api_token

# HashiCorp Vault Configuration (for Vault node)
VAULT_ADDR=http://vault:8200
VAULT_TOKEN=vault_token
VAULT_NAMESPACE=vault_namespace

# JFrog Artifactory Configuration
ARTIFACTORY_URL=http://artifactory:8081
ARTIFACTORY_USERNAME=artifactory_user
ARTIFACTORY_PASSWORD=artifactory_password

# Ansible AWX Configuration
AWX_HOST=http://awx:8043
AWX_USERNAME=awx_user
AWX_PASSWORD=awx_password

# Notification Settings
SLACK_WEBHOOK_URL=slack_webhook_url
MS_TEAMS_WEBHOOK_URL=teams_webhook_url

# Logging
LOG_LEVEL=info
LOG_OUTPUT=file
LOG_FILE_PATH=logs/opflow.log

# Docker/Container Settings
DOCKER_HOST=unix:///var/run/docker.sock
DOCKER_NETWORK=opflow-network